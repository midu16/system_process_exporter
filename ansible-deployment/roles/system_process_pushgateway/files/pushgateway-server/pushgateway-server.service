[Unit]
Description=Pushgateway service
Wants=network-online.target
After=network-online.target

[Service]
User=pushgateway
Group=pushgateway
Type=simple
ExecStart=/usr/local/bin/pushgateway --log.level="info" --web.telemetry-path="/metrics" --web.listen-address=":57005"

[Install]
WantedBy=multi-user.target
